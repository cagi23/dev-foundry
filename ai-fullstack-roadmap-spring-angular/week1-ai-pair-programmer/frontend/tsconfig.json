{
  "compilerOptions": {
    "target": "es2022",
    "useDefineForClassFields": false,
    "module": "es2022",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "importHelpers": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}